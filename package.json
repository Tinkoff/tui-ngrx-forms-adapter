{
    "name": "@tinkoff/tui-ngrx-forms-adapter",
    "version": "1.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "dev:ssr": "ng run demo:serve-ssr",
        "build:ssr": "ng build --prod && ng run demo:server:production",
        "prerender": "ng run demo:prerender",
        "build": "ng build",
        "build:schematics": "tsc -p tsconfig.schematics.json",
        "serve:prerender": "cd dist/demo/browser && http-server",
        "compile:server": "tsc -p ./projects/demo/tsconfig.ssr.json",
        "generate:prerender": "node dist/demo/ssr/prerender",
        "test": "ng test --code-coverage=true",
        "preadd": "npm run build:schematics",
        "add": "schematics ./schematics/collection.json:library-starter --debug=false",
        "postadd": "git add ./projects",
        "lint": "ng lint",
        "lint:less": "stylelint '**/*.less'",
        "typecheck": "tsc --noEmit --skipLibCheck",
        "release": "standard-version",
        "release:patch": "npm run release -- --release-as patch",
        "release:minor": "npm run release -- --release-as minor",
        "release:major": "npm run release -- --release-as major",
        "publish": "npm run build:all && npm run publish:all",
        "build:all": "npm run build:tui-ngrx-forms-adapter",
        "publish:all": "npm run publish:tui-ngrx-forms-adapter",
        "build:tui-ngrx-forms-adapter": "ng run tui-ngrx-forms-adapter:build",
        "test:tui-ngrx-forms-adapter": "ng run tui-ngrx-forms-adapter:test",
        "test:tui-ngrx-forms-adapter:watch": "ng run tui-ngrx-forms-adapter:test --watch=true",
        "publish:tui-ngrx-forms-adapter": "npm publish ./dist/tui-ngrx-forms-adapter"
    },
    "description": "This is a template project to develop, create and open-source Angular libraries with demos",
    "keywords": ["angular", "ng", "workspace", "library", "starter", "tinkoff"],
    "license": "Apache-2.0",
    "author": {
        "name": "Makar Loshakov",
        "email": "m.loshakov@tinkoff.ru"
    },
    "contributors": [],
    "repository": "",
    "bugs": "",
    "homepage": "",
    "schematics": "./schematics/collection.json",
    "dependencies": {
        "@angular/animations": "^10.2.5",
        "@angular/common": "^10.2.5",
        "@angular/compiler": "^10.2.5",
        "@angular/core": "^10.2.5",
        "@angular/forms": "^10.2.5",
        "@angular/localize": "^10.2.5",
        "@angular/platform-browser": "^10.2.5",
        "@angular/platform-browser-dynamic": "^10.2.5",
        "@angular/platform-server": "^10.2.5",
        "@angular/router": "^10.2.5",
        "@ng-web-apis/common": "^1.9.0",
        "@ng-web-apis/universal": "^1.9.2",
        "@ngrx/store": "^9.2.1",
        "@nguniversal/express-engine": "^9.1.1",
        "@taiga-ui/cdk": "^1.6.5",
        "@taiga-ui/core": "^1.6.5",
        "@taiga-ui/icons": "^1.6.5",
        "@taiga-ui/kit": "^1.6.5",
        "core-js": "^2.6.9",
        "express": "^4.15.2",
        "ngrx-forms": "^6.3.5",
        "rxjs": "~6.6.3",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1002.3",
        "@angular-devkit/build-ng-packagr": "~0.1002.3",
        "@angular-devkit/core": "^10.2.3",
        "@angular-devkit/schematics": "^10.2.3",
        "@angular-devkit/schematics-cli": "~0.901.13",
        "@angular/cli": "^10.2.3",
        "@angular/compiler-cli": "^10.2.5",
        "@angular/language-service": "^10.2.5",
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@nguniversal/builders": "^9.1.1",
        "@tinkoff/linters": "^0.6.0",
        "@types/express": "^4.17.0",
        "@types/jasmine": "^3.6.2",
        "@types/jasminewd2": "^2.0.8",
        "@types/node": "12.19.9",
        "coveralls": "^3.1.0",
        "husky": "^4.3.6",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "lint-staged": "^10.5.3",
        "ng-mocks": "^12.3.1",
        "ng-packagr": "^11.2.4",
        "prettier": "^2.2.1",
        "standard-version": "^9.0.0",
        "stylelint": "^10.0.1",
        "ts-mockito": "^2.6.1",
        "ts-node": "^9.1.1",
        "tslint": "~6.1.0",
        "tsutils": "^3.17.1",
        "typescript": "~4.0.8"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged && npm run typecheck"
        }
    },
    "lint-staged": {
        "*.{js,ts,html,md,less,json}": ["prettier --write", "git add"],
        "*.ts": "tslint --fix"
    },
    "standard-version": {
        "scripts": {
            "postbump": "node scripts/syncVersions.js && git add **/package.json"
        }
    },
    "engines": {
        "node": ">= 10",
        "npm": ">= 3"
    }
}
